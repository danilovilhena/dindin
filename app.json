{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "##121212",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dindin.app"
    },
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./assets/images/icon.png",
    "ios": {
      "supportsTablet": true
    },
    "name": "Dindin",
    "newArchEnabled": true,
    "orientation": "portrait",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#121212",
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DMSans-Thin.ttf",
            "./assets/fonts/DMSans-ExtraLight.ttf",
            "./assets/fonts/DMSans-Light.ttf",
            "./assets/fonts/DMSans-Regular.ttf",
            "./assets/fonts/DMSans-Medium.ttf",
            "./assets/fonts/DMSans-SemiBold.ttf",
            "./assets/fonts/DMSans-Bold.ttf",
            "./assets/fonts/DMSans-ExtraBold.ttf",
            "./assets/fonts/DMSans-Black.ttf"
          ]
        }
      ]
    ],
    "scheme": "dindin",
    "slug": "Dindin",
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/icon.png",
      "output": "static"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dca9c5aa-468a-4781-989f-633d44f47e12"
      }
    }
  }
}
